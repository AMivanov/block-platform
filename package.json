  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/react-router-dom": "^5.3.3",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
